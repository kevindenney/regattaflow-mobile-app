{
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "npm install",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/regattaflow",
      "destination": "/regattaflow/index.html"
    },
    {
      "source": "/regattaflow/:path*",
      "destination": "/regattaflow/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/regattaflow",
      "permanent": false
    }
  ]
}
